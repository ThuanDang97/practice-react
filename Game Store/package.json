    "jest": "^29.1.2",
    "jest-environment-jsdom": "^29.1.2",
    "jest-fetch-mock": "^3.0.3",
    "json-server": "^0.17.0",
    "json-server-auth": "^2.1.0",
    "prettier": "^2.7.1",
    "ts-jest": "^29.0.3",
    "tsconfig-paths-webpack-plugin": "^4.0.0",
    "typescript": "^4.6.4",
    "vite": "^3.1.0",
    "vite-plugin-environment": "^1.1.2",
    "vite-tsconfig-paths": "^3.5.1"
  }
